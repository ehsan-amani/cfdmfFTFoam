FoamFile
{
    version         9.0;
    format          ascii;
    class           dictionary;
    object          sampleDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type            probes;

interpolationScheme cellPoint;

libs            ("libsampling.so");

setFormat     raw;

fields
(
    p
);

probeLocations
(
    (0.0125 0.0125 0.0125)
    (0.00375 0.00375 0.00375)
	(0.00375 0.00375 0.02125)
	(0.00375 0.02125 0.00375)
	(0.00375 0.02125 0.02125)
	(0.02125 0.00375 0.00375)
	(0.02125 0.00375 0.02125)
	(0.02125 0.02125 0.00375)
	(0.02125 0.02125 0.02125)
);

