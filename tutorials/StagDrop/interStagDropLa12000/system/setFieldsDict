FoamFile
{
    version     9.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 0
);

regions
(
	sphereToCell
	{
		centre (1.0 1.0 1.0);
		radius        0.4;
		fieldValues
		(
			volScalarFieldValue alpha.water 1
		);
	}
);


