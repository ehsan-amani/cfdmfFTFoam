FoamFile
{
    version         9.0;
    format          ascii;
    class           dictionary;
    object          sampleDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type            probes;

interpolationScheme cellPoint;

libs            ("libsampling.so");

setFormat     raw;

fields
(
    p
);

probeLocations
(
    (1 1 1)
    (0.3 0.3 0.3)
	(0.3 0.3 1.7)
	(0.3 1.7 0.3)
	(0.3 1.7 1.7)
	(1.7 0.3 0.3)
	(1.7 0.3 1.7)
	(1.7 1.7 0.3)
	(1.7 1.7 1.7)
);

