FoamFile
{
    version     9.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default Euler;
}

gradSchemes
{
    default         Gauss linear;
}

divSchemes
{
    div(rhoPhi,U)  Gauss upwind;
    div(densityIGrad)          Gauss linear;
    div(viscosityIGrad)          Gauss linear;
    div(IndicatorGrad)          Gauss linear;
    div(((2*mu)*symm(grad(U))))  Gauss linear;
    div(((1-BB)*IndicatorGrad))   Gauss linear;
    div(phi,alpha)  Gauss vanLeer;
    div(phirb,alpha) Gauss interfaceCompression;
    div(((rho*nuEff)*dev2(T(grad(U))))) Gauss linear;
   
}

laplacianSchemes
{
    default    Gauss linear corrected;
    laplacian(viscosityIndicator_)   Gauss  linear orthogonal;
    laplacian(densityIndicator_)   Gauss  linear orthogonal;
    laplacian(Indicator_)   Gauss  linear orthogonal;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         corrected;
}

fluxRequired
{
    default         no;
    p_rgh;
    pcorr;
    alpha.water;
}


// ************************************************************************* //
