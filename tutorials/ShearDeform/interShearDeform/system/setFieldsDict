FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 1
);

regions
(
 sphereToCell
  {
   centre (0 0 0.6);
   radius        0.3;
  fieldValues
  (
    volScalarFieldValue alpha.water 0
    //volVectorFieldValue U (0 0 -0.23)
   );
   }
);


