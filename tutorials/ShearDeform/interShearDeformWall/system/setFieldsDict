FoamFile
{
    version     9.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 1
);

regions
(
 sphereToCell
  {
   centre (0 0 1.2);
   radius        0.9;
  fieldValues
  (
    volScalarFieldValue alpha.water 0
   );
   }
);


