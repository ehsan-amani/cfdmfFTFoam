FoamFile
{
    version     9.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    walls
    {
    	
    	type		fixedValue;
    	value		uniform (0 0 0);        
    }
    
    inlet
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    
    outlet
    {
        /*
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0);
        */
        //type            zeroGradient;
        type            fixedValue;
        value           uniform (0 0 0);
    }

}


