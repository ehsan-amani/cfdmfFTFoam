FoamFile
{
    version     9.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }

    outlet
    {
        type		fixedValue;
		value           $internalField;
    }

    wall
    {
        type            fixedFluxPressure;  //Same as bouyantPressure in older versions
        value           uniform 0;
    }

}


// ************************************************************************* //
