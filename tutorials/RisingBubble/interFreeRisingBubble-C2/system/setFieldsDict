FoamFile
{
    version     9.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 1
);

regions
(
 sphereToCell
  {
	centre (0.1044 0.1044 0.0522);
	radius        0.01305;
	fieldValues
	(
		volScalarFieldValue alpha.water 0
	);
   }
);


