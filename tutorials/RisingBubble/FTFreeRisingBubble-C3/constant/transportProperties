FoamFile
{
    version     9.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (water air);

water
{
    transportModel  Newtonian;
    nu              nu [0 2 -1 0 0 0 0] 4e-04;
    rho             rho [1 -3 0 0 0 0 0] 1350;
}

air
{
    transportModel  Newtonian;
    nu              nu [0 2 -1 0 0 0 0] 1.453e-05;
    rho             rho [1 -3 0 0 0 0 0] 1.225;
}

sigma           sigma [1 0 -2 0 0 0 0] 0.078;


